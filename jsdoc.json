{
  "plugins": ["jsdoc-http-plugin"],
  "source": {
    "include": ["config", "controllers", "helpers", "middlewares", "models", "routes"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./docs"
  }
}